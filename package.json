{
  "name": "app",
  "version": "0.1.0",
  "dependencies": {
    "jquery": "^3.3.1",
    "jsql-superagent": "^0.3.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1",
    "superagent": "^5.0.5"
  },
  "scripts": {
    "serve": "concurrently --kill-others \"grunt dev\" \"react-scripts start\" \"npm run jsql-watch\"",
    "jsql-watch": "jsql-cli --debug --apiKey=dawid.senko-react@jsql.it --input=/src/**/*.js --devKeyFileName=test-key.key --env=local --watch --development",
    "jsql-build": "jsql-cli --debug --apiKey=dawid.senko-react@jsql.it --output=/build/static/js/main.[...].chunk.js --input=/build/static/js/main.[...].chunk.js --devKeyFileName=test-key.key --env=test",
    "build": "grunt && react-scripts build && npm run jsql-build",
    "react-serve": "react-scripts start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.1.0",
    "grunt": "^1.0.4",
    "grunt-concurrent": "^2.3.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-preprocess": "^5.1.0"
  }
}
